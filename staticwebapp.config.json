{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif,svg}",
            "/css/*"
        ]
    },
    "routes": [
        {
            "route": "/*.{js,css,png,jpg,gif,svg,ico,json}",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/index.html",
            "headers": {
                "cache-control": "no-cache"
            }
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    },
    "globalHeaders": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block"
    },
    "mimeTypes": {
        ".json": "application/json",
        ".js": "text/javascript",
        ".css": "text/css"
    }
}